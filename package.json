{
  "name": "fast-food-fast",
  "version": "1.0.0",
  "description": "Fast-Food-Fast is a food delivery service app for a restaurant. A user can create an account, signin to the account ame make orders for food items.",
  "main": "app.js",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "dev-start": "nodemon --exec babel-node app.js",
    "lint": "eslint app.js",
    "start": "babel-node app.js",
    "test": "mocha server/tests/*.js --exit --require babel-register --reporter spec --timeout=50000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidshare/Fast-Food-Fast.git"
  },
  "author": "David Essien",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/davidshare/Fast-Food-Fast/issues"
  },
  "homepage": "https://github.com/davidshare/Fast-Food-Fast#readme",
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.4"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-register": "^6.26.0",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "express": "^4.16.3",
    "nyc": "^13.0.1"
  }
}
